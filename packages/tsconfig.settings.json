{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["es2018"],
    "module": "esnext",
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext"
  },
  "references": [
    {
      "@minorm/core": "core"
    },
    {
      "@minorm/schema-tool": "schema-tool"
    }
  ]
}
