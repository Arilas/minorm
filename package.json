{
  "name": "minorm",
  "version": "1.0.0-next.1",
  "description": "Minorm - Light high-performance ORM for MySQL",
  "main": "lib/index.js",
  "scripts": {
    "dist": "babel --extensions .ts -d lib/ src/",
    "prepublish": "in-publish && npm run dist || echo 'Bad mode'",
    "lint": "eslint --ext .ts .",
    "test": "jest --runInBand",
    "test:integration": "jest test/integration --runInBand",
    "test:unit": "jest test/unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arilas/minorm.git"
  },
  "keywords": [
    "MinORM",
    "Krona",
    "MySQL",
    "ORM"
  ],
  "author": "Alex Grand <alex@granted.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arilas/minorm/issues"
  },
  "homepage": "https://github.com/Arilas/minorm#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^12.0.0",
    "array-aggregate": "^0.1.7",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "flow-typed": "^2.5.1",
    "in-publish": "^2.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "sinon": "^7.3.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/jest": "^24.0.12",
    "@types/sinon": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "is-generator-function": "^1.0.7",
    "mysql2": "^1.6.5",
    "squel": "^5.12.0"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scriptslib/)[/\\\\]",
      "(config|fixtures)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ]
  }
}
