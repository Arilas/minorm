{
  "name": "minorm",
  "version": "0.2.3-next.1",
  "description": "MinORM - Light high-performance ORM for MySQL",
  "main": "lib/index.js",
  "scripts": {
    "dist": "babel -d lib/ src/",
    "prepublish": "in-publish && npm run dist || echo 'Bad mode'",
    "lint": "eslint .",
    "test": "jest --runInBand",
    "test:integration": "jest test/integration --runInBand",
    "test:unit": "jest test/unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arilas/minorm.git"
  },
  "keywords": [
    "MinORM",
    "Krona",
    "MySQL",
    "ORM"
  ],
  "author": "Alex Grand <alex@granted.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arilas/minorm/issues"
  },
  "homepage": "https://github.com/Arilas/minorm#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.0",
    "array-aggregate": "^0.1.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-prettier": "^3.0.1",
    "flow-bin": "^0.97.0",
    "flow-typed": "^2.5.1",
    "in-publish": "^2.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.17.2",
    "is-generator-function": "^1.0.7",
    "mysql2": "^1.6.5",
    "squel": "^5.12.0"
  },
  "engines": {
    "node": ">=6.10"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scriptslib/)[/\\\\]",
      "(config|fixtures)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ]
  }
}
